{
  "name": "auth0-authentication",
  "description": "A simple Auth0 authenticated React client and Node/Express server.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "typecheck": "tsc",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:ls": "ls-lint",
    "format": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.10.1",
    "classnames": "^2.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.3.0",
    "react-spinners": "^0.12.0",
    "rooks": "^5.11.0"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "react-scripts": "^5.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
